{
  "name": "similo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start -p 3026",
    "prod:start": "pm2 start yarn --name \"similo\" --interpreter bash -- start",
    "lint": "eslint ./ --ext js,jsx",
    "lint:fix": "eslint ./ --ext js,jsx --fix",
    "migrate": "npx sequelize db:migrate && npx sequelize db:seed:all",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "seed:undo": "npx sequelize db:seed:undo",
    "migration": "npx sequelize migration:generate --name",
    "seed": "npx sequelize seed:generate --name",
    "model": "npx sequelize model:create --name"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dotenv-flow": "^3.2.0",
    "next": "10.2.3",
    "next-auth": "^3.26.1",
    "pg": "^8.6.0",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.34.1",
    "sequelize": "^5.22.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext js,jsx --fix --ignore-pattern",
      "git add"
    ]
  }
}
