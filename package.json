{
  "name": "similo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "prod:start": "NODE_ENV=production pm2 start server.js --name \"similo-next\"",
    "prod:restart": "yarn build && pm2 restart \"similo-next\"",
    "prod:migrate": "npx sequelize db:migrate",
    "lint": "eslint ./ --ext js,jsx",
    "lint:fix": "eslint ./ --ext js,jsx --fix",
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "seed:undo": "npx sequelize db:seed:undo",
    "migration": "npx sequelize migration:generate --name",
    "seed": "npx sequelize seed:generate --name",
    "model": "npx sequelize model:create --name"
  },
  "dependencies": {
    "@passport-next/passport": "^3.1.0",
    "array-move": "^3.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-random-string": "^4.0.0",
    "csv-parser": "^3.0.0",
    "dotenv-flow": "^3.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-sequelize-crud": "^6.1.5",
    "fs-extra": "^10.0.0",
    "generate-password": "^1.6.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "next": "10.2.3",
    "next-auth": "^3.26.1",
    "next-connect": "^0.10.1",
    "node-cron": "^3.0.0",
    "nodejs-file-downloader": "^4.7.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "psl": "^1.8.0",
    "qs": "^6.10.1",
    "rand-token": "^1.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-modal": "^3.14.3",
    "react-tooltip": "^4.2.21",
    "react-truncate": "^2.4.0",
    "react-useportal": "^1.0.14",
    "response-time": "^2.3.2",
    "sass": "^1.34.1",
    "sequelize": "^6.6.2",
    "url": "^0.11.0",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext js,jsx --fix --ignore-pattern",
      "git add"
    ]
  }
}
